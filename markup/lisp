||[#defclass define class]||(defclass rectangle () _
@<&nbsp;&nbsp;>@( _
@<&nbsp;&nbsp;>@@<&nbsp;&nbsp;>@(height _
@<&nbsp;&nbsp;>@@<&nbsp;&nbsp;>@@<&nbsp;&nbsp;>@:accessor rectangle-height _
