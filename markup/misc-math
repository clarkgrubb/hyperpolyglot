set output "boxplot.png" _
set style boxplot outliers pointtype 7 _
set style data boxplot _
set boxwidth 0.2 _
plot [0.5:1.5] [0.0:1.1] "data.txt" \ _
@<&nbsp;&nbsp;>@using (1.0):1:(0.3) notitle|| ||
